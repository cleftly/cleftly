{
    "name": "frontend",
    "version": "0.1.0",
    "license": "MIT",
    "private": true,
    "scripts": {
        "tauri": "tauri",
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --check . && eslint .",
        "format": "prettier --write . && eslint . --fix",
        "cargo": "{ cd src-tauri && cargo }",
        "prepare": "husky install"
    },
    "devDependencies": {
        "@skeletonlabs/skeleton": "^2.10.3",
        "@skeletonlabs/tw-plugin": "^0.2.4",
        "@sveltejs/adapter-static": "^3.0.6",
        "@sveltejs/kit": "^2.9.0",
        "@sveltejs/vite-plugin-svelte": "^3.1.2",
        "@tauri-apps/cli": "~2.1.0",
        "@types/qs": "^6.9.17",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "autoprefixer": "^10.4.20",
        "eslint": "^8.57.1",
        "eslint-config-prettier": "^8.10.0",
        "eslint-import-resolver-typescript": "^3.6.3",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-svelte": "^2.46.1",
        "eslint-plugin-unused-imports": "^3.2.0",
        "husky": "^8.0.3",
        "lint-staged": "^15.2.10",
        "postcss": "^8.4.49",
        "postcss-load-config": "^4.0.2",
        "prettier": "^2.8.8",
        "prettier-plugin-svelte": "^2.10.1",
        "rollup-plugin-visualizer": "^5.12.0",
        "svelte": "^4.2.19",
        "svelte-check": "^3.8.6",
        "tailwindcss": "^3.4.15",
        "tslib": "^2.8.1",
        "typescript": "^5.7.2",
        "vite": "^5.4.11",
        "vite-plugin-node-polyfills": "^0.21.0"
    },
    "type": "module",
    "dependencies": {
        "@floating-ui/dom": "^1.6.12",
        "@tauri-apps/api": "~2.1.1",
        "@tauri-apps/plugin-dialog": "~2.0.1",
        "@tauri-apps/plugin-fs": "~2.0.3",
        "@tauri-apps/plugin-http": "~2.0.1",
        "@tauri-apps/plugin-os": "~2.0.0",
        "@tauri-apps/plugin-process": "~2.0.0",
        "@tauri-apps/plugin-shell": "~2.0.1",
        "@tauri-apps/plugin-updater": "~2.0.0",
        "axios": "^1.7.8",
        "axios-tauri-api-adapter": "^2.0.3",
        "comlink": "^4.4.2",
        "dexie": "3.2.4",
        "fuse.js": "^7.0.0",
        "js-md5": "^0.8.3",
        "lucide-svelte": "^0.288.0",
        "music-metadata-browser": "^2.5.11",
        "pino": "^8.21.0",
        "qs": "^6.13.1",
        "svelte-i18n": "^4.0.1",
        "tauri-plugin-context-menu": "^0.7.0",
        "tauri-plugin-log-api": "github:tauri-apps/tauri-plugin-log#v2"
    },
    "lint-staged": {
        "**/*.{js,ts,svelte}": [
            "eslint --fix"
        ],
        "**/*.{js,ts,svelte,css,md,html,json,yaml,yml,jsx,tsx}": [
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    }
}
